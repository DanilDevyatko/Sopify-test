{% for collection in collections %}
<a href="{{ collection.url }}">
  <div class="img-wrap">
    <img src=" {{ collection.image | img_url: '100x100'}} " alt="">
  </div>
  <h2 class="titile">
    {{ collection.title }}
  </h2>
  <div class="description">
    {{ collection.description }}  
  </div>
</a>
{% endfor %}